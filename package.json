    },
    "importSort": {
        ".js, .jsx, .ts, .tsx": {
            "style": "module"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "git-format-staged -f 'prettier --stdin-filepath \"{}\"' '*.js' '*.jsx' '*.ts' '*.tsx' '*.scss' '*.json' '*.gql', '*.md'"
        }
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
